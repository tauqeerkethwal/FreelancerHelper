    <script type="text/javascript">



        $(document).ready(function () {



            InitDatePicker();

        });


        function InitDatePicker()
        {

            $('.datePicker').datepicker({
                format: 'dd-mm-yyyy',
                language: "da-DK",
                weekStart: 1,
                //format: 'dd-mm-yyyy',
                changeMonth: true,
                changeYear: true,
                autoclose: true,
                todayHighlight: true,
                onSelect: function(dateText) {
                    if (typeof loadInterpreter == 'function') {
                        loadInterpreter();
                    }
                }
            });


        }

        function openConfirmationDialog(message, callbackMethod, params) {

            if (!message)
                message = "Are you sure!";

            $('#confirmationModel_Body').html(message);
            $('#confirmationModel').modal('show');

            $("#confirmationModel_OkayButton").off('click').on('click', function (e) {

                hideConfirmationDialog();

                if (callbackMethod) {
                    callbackMethod(params);
                }

            });

        }
        function openSuccessMessageDialog(message, callbackMethod, params) {

            if (!message)
                message = "fuldført";

            $('#SuccessMessageModel_Body').html(message);
            $('#SuccessMessageModel').modal('show');

            $("#SuccessMessageModel_OkayButton").off('click').on('click', function (e) {

                hideSuccessMessageDialog();

                if (callbackMethod) {
                    callbackMethod(params);
                }

            });

        }

        function hideConfirmationDialog() {
            $('#confirmationModel').modal('hide');
        }

        function hideSuccessMessageDialog() {
            $('#SuccessMessageModel').modal('hide');
        }

    </script>

